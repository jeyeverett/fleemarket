<% if (errorMessage.length) { %> 
    <div class="user-msg user-msg--error">
        <% errorMessage.forEach(error => { %> 
            <% console.log(error?.msg); %>
            <%= error?.msg %><br>
        <% }); %> 
    </div>
<% } else if (successMessage.length) { %> 
    <div class="user-msg"><%= successMessage %> </div>
<% } %> 